@{
    ViewBag.Title = "Get Processed Order";
}

<h2>Get Processed Order</h2>

<form asp-action="GetProcessedOrder" asp-controller="Order" method="get">
    <div>
        <input type="submit" value="Retrieve Order" />
    </div>
</form>

@if (ViewBag.Message != null)
{
    <div>
        <p>@ViewBag.Message</p>
        </div>
    }
}
